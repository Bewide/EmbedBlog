<% if (list.length>0) { %>
<ul class="icards" style="margin: 12px -8px;">
	<% list.forEach(function(e){ %>
		<li class="w20" style="cursor: pointer;" data-action="select" data-id="<%=e.filename%>">
			<div style="background-color:#ddd;height: 100px;background-image: url('../img/tn/<%=e.filename%>');background-repeat: no-repeat;background-size: cover;background-position: center center;"></div>
		</li>
	<% }); %>
</ul>
<% } else { %>
<div style="text-align: center;margin: 3em 0;font-size: 20px;color:#9e9e9e;">
	Empty
</div>
<% } %>