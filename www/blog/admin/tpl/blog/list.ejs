<% if (list && list.length>0) { %>
<table class="table bordered rounded">
	<thead>
		<tr>
			<th style="width:20px;"></th>
			<th>Title</th>
			<th class="right">Created</th>
			<th class="right">Updated</th>
		</tr>
	</thead>
	<tbody>
		<% list.forEach(function(e){ %>
		<tr>
			<td class="center"><span style="position:relative;top:-2px;color:<%=e.published==1?'#00bf00':'#ff0076'%>;">&#11044;</span></td>
			<td><a href="#/blog/edit/<%=e.id%>/"><%=e.title%></a></td>
			<td class="sm right"><%=moment(e.time_created).format('ddd MMM Do, HH:mm')%></td>
			<td class="sm right"><%=moment(e.time_updated).format('ddd MMM Do, HH:mm')%></td>
		</tr>
		<% }); %>
	</tbody>
</table>
<% } else { %>

<div class="emptymsg">
	No blog posts found
</div>

<% } %>